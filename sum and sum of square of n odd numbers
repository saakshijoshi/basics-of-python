      "source": [
        "def squaresum(n) :\n",
        "  sm = 0\n",
        "  for i in range(1, n+1) :\n",
        "      sm += (2*i-1) *(2*i-1)\n",
        "  return sm\n",
        "n = 10\n",
        "print(squaresum(10))\n"
      ],
